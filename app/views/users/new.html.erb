  <div class="user_container">

    <div class="signup">
      <%= form_for :user, url: '/users' do |f| %>

      <div class="form-label">Sign Up</div>

      <div class="form-input">
        <%= f.text_field :name, placeholder: "Name", class: "form-control"%>
      </div>

      <div class="form-input">
        <%= f.text_field :email, placeholder: "Email", class: "form-control"%>
      </div>

      <div class="form-input">
        <%= f.password_field :password, placeholder: "Password", class: "form-control"%>
      </div>

      <div class="form-input">
        <%= f.password_field :password_confirmation, placeholder: "Confirm Password", class: "form-control"%>
      </div>
      <div class="errors"></div>

      <div class="form-input">
        <%= f.submit "Submit", class: "submit btn btn-default" %>
      </div>
      <% end %>
    </div>


    <div class="login">



      <%= form_for :login, url: '/login' do |f|%>

      <div class="form-label">Login</div>

      <div class="form-input">
        <%= f.text_field :email, placeholder: "Email", class: "form-control" %>
      </div>
      <div class="form-input">
        <%= f.password_field :password, placeholder: "Password", class: "form-control"%>
      </div class="form-input">
      <div>
        <%= f. submit "Submit", class: "btn btn-default"%>
      </div>

      <% end %>
    </div>
  </div>


</div>


